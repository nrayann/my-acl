{
    "name": "nrayann/my-acl",
    "description": "MyAcl plugin for CakePHP",
    "type": "cakephp-plugin",
    "homepage": "https://github.com/nrayann",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "cakephp/acl": "^0.2.2"
    },
    "keywords": [
        "cakephp", "cakephp3", "plugin", "acl"
    ]
    "authors": [
        {
            "name": "Rayann Nayran",
            "email": "rayannsi@me.com",
            "homepage": "https://github.com/nrayann",
            "role": "Developer"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "MyAcl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyAcl\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
